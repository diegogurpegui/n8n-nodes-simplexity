{
  "name": "n8n-nodes-simplexity",
  "version": "0.1.4",
  "description": "n8n nodes for SimpleX Chat integration",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-custom-node",
    "n8n-community-node-package",
    "simplex",
    "chat",
    "messaging"
  ],
  "license": "MIT",
  "homepage": "https://github.com/diegogurpegui/n8n-nodes-simplexity.git",
  "author": {
    "name": "Diego H. Gurpegui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegogurpegui/n8n-nodes-simplexity.git"
  },
  "bugs": {
    "url": "https://github.com/diegogurpegui/n8n-nodes-simplexity/issues"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "bundle": "npm run build && rm -rf bundle && mkdir -p bundle && cp -r dist package.json README.md LICENSE bundle/ && echo 'Bundle created successfully at ./bundle/'"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SimplexityApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Simplexity/SimplexityTrigger.node.js",
      "dist/nodes/Simplexity/Simplexity.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.32.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "globals": "^16.3.0",
    "gulp": "^5.0.1",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "simplex-chat": "^0.2.1"
  }
}
